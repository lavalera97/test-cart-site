packages:
  yum:
    postgresql93-devel: []
    libjpeg-devel: '6b'

container_commands:
    01_migrate:
        command: "django-admin.py migrate"
        leader_only: true
    02_createsuperuser:
        command: "echo \"from accounts.models import Account; Account.objects.create_superuser('first_name', 'last_name', 'your-email@gmail.com', 'yourusername', 'password')\" | python manage.py create_superuser"
        leader_only: true
option_settings:
    aws:elasticbeanstalk:application:environment:
        DJANGO_SETTINGS_MODULE: greatkart.settings